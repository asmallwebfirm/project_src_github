<?php

/**
 * @file
 * Install and update hooks for the Project Source: GitHub module.
 */


/**
 * Implements hook_uninstall().
 */
function project_src_github_uninstall() {
  $variables = array(
    'project_src_github_org',
  );

  foreach ($variables as $variable) {
    variable_del($variable);
  }
}
